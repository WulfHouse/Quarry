# Test 10: Function Definitions and Calls

fn add(a: int, b: int) -> int:
    return a + b

fn multiply(a: int, b: int) -> int:
    return a * b

fn greet(name: &str):
    print("Hello,", name)

fn main():
    let sum = add(5, 3)
    print("Sum:", sum)
    
    let product = multiply(4, 7)
    print("Product:", product)
    
    greet("Pyrite")

